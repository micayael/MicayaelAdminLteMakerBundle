{
    "name": "micayael/admin-lte-maker-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Ardissone",
            "email": "micayael@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "~4.3",
        "symfony/dependency-injection": "~4.3",
        "symfony/orm-pack": "*",
        "doctrine/inflector": "^1.3",
        "symfony/maker-bundle": "^1.14",
        "symfony/twig-pack": "*",
        "symfony/form": "~4.3",
        "symfony/validator": "~4.3",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/security": "~4.3",
        "twig/twig": "2.*",
        "knplabs/knp-paginator-bundle": "^4.1",
        "symfony/translation": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Micayael\\AdminLteMakerBundle\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "sensiolabs/security-checker": "^6.0"
    },
    "scripts": {
        "check-cs": [
            "php-cs-fixer fix --dry-run"
        ],
        "check-sec": [
            "security-checker -vv security:check"
        ],
        "check": [
            "@check-cs",
            "@check-sec"
        ],
        "fix-cs": [
            "php-cs-fixer fix -vvv"
        ]
    }
}
